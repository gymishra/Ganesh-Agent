{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringLike": {
          "aws:userid": "*:*FileUploadFunction*"
        },
        "StringEquals": {
          "sts:ExternalId": "workshop-sap-genai-2025"
        }
      }
    }
  ]
}
