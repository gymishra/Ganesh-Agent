{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowOwnerFullAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::953841955037:root"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::wrkshp-qbiz-data",
        "arn:aws:s3:::wrkshp-qbiz-data/*"
      ]
    },
    {
      "Sid": "AllowWorkshopLambdaFunctions",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::wrkshp-qbiz-data",
        "arn:aws:s3:::wrkshp-qbiz-data/*"
      ],
      "Condition": {
        "StringLike": {
          "aws:userid": [
            "*:*FileUploadFunction*",
            "*:*workshop*",
            "*:*s3bucket*"
          ]
        }
      }
    }
  ]
}
